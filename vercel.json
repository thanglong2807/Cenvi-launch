{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_BASE_URL": "@next_public_api_base_url",
      "NODE_ENV": "production"
    },
    "framework": "nextjs"
  }
